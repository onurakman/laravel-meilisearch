{
    "name": "onurakman/laravel-meilisearch",
    "description": "Simple Laravel wrapper to use Meilisearch",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^8|^9|^10",
        "illuminate/support": "^8|^9|^10"
    },
    "autoload": {
        "psr-4": {
            "Eelcol\\LaravelMeilisearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eelcol\\LaravelMeilisearch\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eelcol\\LaravelMeilisearch\\LaravelMeilisearchServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
